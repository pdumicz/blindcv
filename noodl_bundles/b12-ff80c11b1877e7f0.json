ame":"File","type":"*","group":"Inputs","plug":"input","index":0},{"name":"Resume ID","type":"*","plug":"output","index":1},{"name":"Do","type":{"name":"signal","allowConnectionsOnly":true},"group":"Actions","plug":"input","index":1},{"name":"Success","type":"signal","plug":"output","index":2},{"name":"Failure","type":"signal","plug":"output","index":3},{"name":"Job Description","type":{"name":"*"},"group":"Inputs","plug":"input","index":3},{"name":"Error Message","type":"*","plug":"output","index":4},{"name":"Internal Assessment","type":"boolean","group":"Inputs","plug":"input","index":4},{"name":"External Assessment","type":"boolean","group":"Inputs","plug":"input","index":5},{"name":"Watermark Path","type":"*","group":"Inputs","plug":"input","index":6},{"name":"Logo Path","type":"*","group":"Inputs","plug":"input","index":7},{"name":"Anonymised","type":"*","group":"Inputs","plug":"input","index":8}],"roots":[]},{"name":"/API/Upload CV File","nodes":[{"id":"1c466178-0563-d4fd-9ac1-72deb05c8c04","type":"Component Inputs","parameters":{},"ports":[],"children":[]},{"id":"8aa354b3-c127-ffed-c3c9-d475ef3030b0","type":"Component Outputs","parameters":{},"ports":[],"children":[]},{"id":"fb79c1ca-c2df-6b70-37e6-320690c15f44","type":"JavaScriptFunction","parameters":{"functionScript":"// Initialization\nconst supabase = Noodl.Variables.supabase;\nconst userId = Inputs.User;\nconst file = Inputs.File; // Changed from File to Files to handle multiple files\nconst template = Inputs.Template;\nconst jobDecription = Inputs.JobDescription;\nconst logoPath = Inputs.LogoPath;\nconst watermarkPath = Inputs.WatermarkPath;\nlet includeWatermark = false;\n\nif (watermarkPath) {\n  includeWatermark = true;\n}\n\nconst generateFilename = (file) => {\n  const randomSequence = Math.random().toString(36).substring(2, 15) + Math.random().toString(36).substring(2, 15);\n  const splitFileName = file.name.split('.');\n  const newName = `${splitFileName.slice(0, -1).join('.')}_${randomSequence}.${splitFileName.pop()}`;\n  return newName;\n}\n\nconst uploadFile = async (supabase, userId, file) => {\n  const newFileName = generateFilename(file);\n  const { data: uploadData, error: uploadError } = await supabase.storage\n    .from('resume_files')\n    .upload(`${userId}/${newFileName}`, file);\n  if (uploadError) throw new Error(uploadError.message);\n  return uploadData.path;\n}\n\nconst insertDataToTable = async (supabase, template, filePath, userId) => {\n  const { data: insertData, error: insertError } = await supabase\n    .from('resumes')\n    .insert([{\n      template: template,\n      file_path: filePath,\n      user_id: userId,\n      processing: true,\n      comparison_description: jobDecription,\n      include_watermark: includeWatermark,\n      watermark_path: watermarkPath,\n      image_path: logoPath,\n      anonymised: Inputs.Anonymised\n    }])\n    .select();\n  if (insertError) throw new Error(insertError.message);\n  return insertData;\n}\n\n// Inside your main function call above functions and send outputs appropriately.\nconst mainFunction = async () => {\n  try {\n    const filePath = await uploadFile(supabase, userId, file);\n    Outputs.FilePath = filePath;\n\n    const insertData = await insertDataToTable(supabase, template, filePath, userId);\n    Outputs.ResumeID = insertData[0].id;\n    Outputs.Response = insertData;\n\n    Outputs.Success() // Emit Success signal after all files have been processed\n  } catch (err) {\n    Outputs.Error()\n    Outputs.ErrorMessage = err.message;\n  }\n}\nmainFunction()","scriptInputs":[{"id":"x0v3","label":"fileName"}],"in-fileName":"file1"},"ports":[{"name":"intype-fileName","displayName":"Type","editorName":"fileName | Type","plug":"input","type":{"name":"enum","enums":[{"value":"string","label":"String"},{"value":"boolean","label":"Boolean"},{"value":"number","label":"Number"},{"value":"object","label":"Object"},{"value":"date","label":"Date"},{"value":"array","label":"Array"},{"value":"color"