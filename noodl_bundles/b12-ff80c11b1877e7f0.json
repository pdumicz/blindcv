0rnAVwRetNVxXUXVHQFYyw3iA9rW_ppmJornsslLf8'\nconst body = {\n  filePath: filePath,\n  resumeId: resumeId,\n  internalAssessment: internalAssessment,\n  externalAssessment: externalAssessment,\n  jobDescription: jobDescription\n}\n\n  try {\n    const res = await fetch(url, {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json',\n      },\n      body: JSON.stringify(body),\n    });\n\n    if (!res.ok) {   \n      throw new Error(`HTTP error! status: ${res.status}`); \n    } else {\n      Outputs.Success();\n    }\n  } catch (error) {\n    console.error(error);\n    Outputs.ErrorMessage = error.message;\n    Outputs.Failure();\n  }","scriptInputs":[{"id":"kp8r","label":"InternalAssessment"},{"id":"cr31","label":"ExternalAssessment"}],"in-fileName":"file1","intype-ComparisonOnly":"boolean","intype-InternalAssessment":"boolean","intype-ExternalAssessment":"boolean"},"ports":[{"name":"intype-InternalAssessment","displayName":"Type","editorName":"InternalAssessment | Type","plug":"input","type":{"name":"enum","enums":[{"value":"string","label":"String"},{"value":"boolean","label":"Boolean"},{"value":"number","label":"Number"},{"value":"object","label":"Object"},{"value":"date","label":"Date"},{"value":"array","label":"Array"},{"value":"color","label":"Color"}],"allowEditOnly":true},"default":"string","parent":"scriptInputs","parentItemId":"kp8r","index":4},{"name":"in-InternalAssessment","displayName":"InternalAssessment","plug":"input","type":"boolean","group":"Inputs","index":5},{"name":"intype-ExternalAssessment","displayName":"Type","editorName":"ExternalAssessment | Type","plug":"input","type":{"name":"enum","enums":[{"value":"string","label":"String"},{"value":"boolean","label":"Boolean"},{"value":"number","label":"Number"},{"value":"object","label":"Object"},{"value":"date","label":"Date"},{"value":"array","label":"Array"},{"value":"color","label":"Color"}],"allowEditOnly":true},"default":"string","parent":"scriptInputs","parentItemI