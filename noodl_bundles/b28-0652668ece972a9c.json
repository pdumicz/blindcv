n  Outputs.Records = data; // Send the retrieved data to the Records output\n  Outputs.Success(); // Signal that the operation was successful\n} catch (error) {\n  Outputs.Error = error; // Send the error to the Error output\n  Outputs.Failure(); // Signal that the operation failed\n}\n"},"ports":[{"name":"in-Skip","displayName":"Skip","plug":"input","type":"*","group":"Inputs","index":4},{"name":"in-Limit","displayName":"Limit","plug":"input","type":"*","group":"Inputs","index":5},{"name":"out-Success","displayName":"Success","plug":"output","type":"signal","group":"Outputs","index":6},{"name":"out-Failure","displayName":"Failure","plug":"output","type":"signal","group":"Outputs","index":7},{"name":"out-Count","displayName":"Count","plug":"output","type":"*","group":"Outputs","index":8},{"name":"out-Records","displayName":"Records","plug":"output","type":"*","group":"Outputs","index":9},{"name":"out-Error","displayName":"Error","plug":"output","type":"*","group":"Outputs","index":10}],"children":[]}],"connections":[{"sourceId":"0ad0eaf9-c605-d987-d381-e510604dc176","sourcePort":"Do","targetId":"ab1cac1f-7932-97b5-c43f-d2caa46e5b15","targetPort":"run"},{"sourceId":"ab1cac1f-7932-97b5-c43f-d2caa46e5b15","sourcePort":"out-Success","targetId":"953d531b-87c4-7ba7-e292-f90f9fe66ada","targetPort":"Success"},{"sourceId":"ab1cac1f-7932-97b5-c43f-d2caa46e5b15","sourcePort":"out-Failure","targetId":"953d531b-87c4-7ba7-e292-f90f9fe66ada","targetPort":"Failure"},{"sourceId":"ab1cac1f-7932-97b5-c43f-d2caa46e5b15","sourcePort":"out-Records","targetId":"953d531b-87c4-7ba7-e292-f90f9fe66ada","targetPort":"Records"},{"sourceId":"ab1cac1f-7932-97b5-c43f-d2caa46e5b15","sourcePort":"out-Error","targetId":"953d531b-87c4-7ba7-e292-f90f9fe66ada","targetPort":"Error"},{"sourceId":"0ad0eaf9-c605-d987-d381-e510604dc176","sourcePort":"Limit","targetId":"ab1cac1f-7932-97b5-c43f-d2caa46e5b15","targetPort":"in-Limit"},{"sourceId":"0ad0eaf9-c605-d987-d381-e510604dc176","sourcePort":"Skip","target