ser)\ndocument.body.appendChild(link);\n\n// Programmatically click the link to trigger the download\nlink.click();\n\n// Clean up by removing the link and revoking the object URL\ndocument.body.removeChild(link);\nwindow.URL.revokeObjectURL(url);\n\n"},"ports":[{"name":"in-Blob","displayName":"Blob","plug":"input","type":"*","group":"Inputs","index":4},{"name":"in-Filename","displayName":"Filename","plug":"input","type":"*","group":"Inputs","index":5}],"children":[]},{"id":"1222e213-a7fe-945f-eea3-aa9db2516cb7","type":"Component Inputs","parameters":{},"ports":[],"children":[]}],"connections":[{"sourceId":"1222e213-a7fe-945f-eea3-aa9db2516cb7","sourcePort":"Blob","targetId":"60fca921-011b-04c5-f19b-e9696704574f","targetPort":"in-Blob"},{"sourceId":"1222e213-a7fe-945f-eea3-aa9db2516cb7","sourcePort":"Filename","targetId":"60fca921-011b-04c5-f19b-e9696704574f","targetPort":"in-Filename"},{"sourceId":"1222e213-a7fe-945f-eea3-aa9db2516cb7","sourcePort":"Run","targetId":"60fca921-011b-04c5-f19b-e9696704574f","targetPort":"run"}],"ports":[{"name":"Blob","type":"*","group":"Inputs","plug":"input","index":1},{"name":"Filename","type":"*","group":"Inputs","plug":"input","index":2},{"name":"Run","type":{"name":"signal","allowConnectionsOnly":true},"group":"Actions","plug":"input","index":3}],"roots":[]}]