)\n\nconst errors = supabase.channel('ai-errors')\n  .on(\n    'broadcast',\n    { event: 'error' },\n    (payload) => {\n      Outputs.Error = payload;\n      console.log(payload);\n    }\n  )\n  .subscribe()"},"ports":[{"name":"out-Updated","displayName":"Updated","plug":"output","type":"signal","group":"Outputs","index":4},{"name":"out-Payload","displayName":"Payload","plug":"output","type":"*","group":"Outputs","index":5},{"name":"out-Error","displayName":"Error","plug":"output","type":"*","group":"Outputs","index":6}],"children":[]},{"id":"a85f0ae8-21a4-edae-12e6-b4dcd731a604","type":"/Show Toast","parameters":{"Type":"Success","Message":"Upload successful. Please wait for the CV to finish processing."},"ports":[],"children":[]},{"id":"88993caa-ec0e-ac2d-d36d-67c9865db9ae","type":"JavaScriptFunction","parameters":{"functionScript":"// Create a new file input element\nconst fileInput = document.createElement('input');\n\n// Set the type to file to open the file picker\nfileInput.type = 'file';\n\n// Allow for multiple file selection\nfileInput.multiple = true;\n\n// Listen for the change event which is fired when files are selected\nfileInput.addEventListener('change', (event) => {\n  // Get the selected files from the event\n  const files = event.target.files;\n\n  // If no files were selected, send the Failure signal\n  if (files.length === 0) {\n    Outputs.Failure();\n    return;\n  }\n\n  // Convert the FileList to an array\n  const fileList = Array.from(files);\n\n  // Map the array to an array of objects with file as a property\n  const fileObjects = fileList.map(file => ({file}));\n\n  // Send the file objects to the output\n  Outputs.Files = fileObjects;\n\n  // Send the Success signal\n  Outputs.Success();\n});\n\n// Trigger the file picker\nfileInput.click();\n"},"ports":[{"name":"out-Failure","displayName":"Failure","plug":"output","type":"signal","group":"Outputs","index":4},{"name":"out-Success","displayName":"Success","plug":"output","type":"signa